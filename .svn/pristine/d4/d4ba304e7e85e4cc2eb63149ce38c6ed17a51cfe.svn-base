GV.Buttons.zoomin = function(btnOptions, map) {
    "use strict";

    var action = L.ToolbarAction.extend({

        options: {
            toolbarIcon: {
                html: '+',
                tooltip: 'Zoom In'
            }
        },

        addHooks: function () {
            map.zoomIn();
        }

    });

    return action;

};