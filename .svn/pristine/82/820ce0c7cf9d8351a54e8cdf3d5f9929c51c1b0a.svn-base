#gv-title {
    height: 0;
    width: 0;
}

@media only screen and (min-width: 700px) {
    /* For desktop only: */
    #gv-title {
        position: absolute;
        left: 10px;
        top: 10px;
        height: 94px;
        width: 230px;
        padding-left: 10px;
        z-index: 799;
        background: #fff url(images/logo-ProgettoScuola.png) no-repeat 10% 50%;
        opacity: 1;
        filter: alpha(opacity=100);
    }
}

@media only screen and (min-width: 700px) {
    .leaflet-container .leaflet-control-search {
        top: 94px;
    }
}

#gv-legend {
    height: auto;
}

#gv-legend-body {
    height: auto;
    overflow-y: hidden;
}

.gv-legend-layer-icon {
    width: 24px;
    height: 27px;
}
.gv-list-legend-map-item {
    height: 0;
    padding: 0;
}

