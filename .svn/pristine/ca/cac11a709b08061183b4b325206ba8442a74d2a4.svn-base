        "use strict";
        this.on('zoom', function () {
            var layers = GV.rlMaps.getAllLayersConfig();
            _.each(layers, function (layer) {
                GV.rlMaps.setLayerAttribute(layer.name, 'inRange', this.layerInRange(layer));
            }, this);
        });
    },

